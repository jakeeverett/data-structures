#include<iostream>
#include<string>

template<class ItemType>
class QueueType{
Public:
QueueType(int i);
~QueueType();
void MakeEmpty();
bool IsEmpty() const;
bool IsFull() const;
void Enqueue(ItemType);
void Dequeue(ItemType&);
Private:
int front, rear;
ItemType* items;
int maxQue;
};
